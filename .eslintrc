{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": []
      }
    ],
    "react/forbid-prop-types": [1, { "forbid": ["any"] }],
    "no-underscore-dangle": ["error", { "allow": ["_data"] }],
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "max-len": [1, { "code": 150 }]
  }
}
